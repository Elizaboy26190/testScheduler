{
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"rules": {
		"eqeqeq": 2,
		"no-caller": 2,
		"no-extend-native": 2,
		"no-irregular-whitespace": 2,
		"no-loop-func": 1,
		"no-multi-spaces": 1,
		"no-undef": 2,
		"no-unused-vars": 2,
		"no-var": 2,
		"one-var": [2, "never"],
		"quotes": [1, "single"],
		"semi": [1, "always"],
		"curly": [1, "multi-line"],
		"space-before-function-paren": [1, "always"],
		"wrap-iife": 2
	}
}